<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RecordButton">
        <attr name="bg_color" format="color" />
        <attr name="stroke_color" format="color" />
        <attr name="stroke_width" format="dimension" />
        <attr name="duration_0" format="integer" />
        <attr name="radius" format="dimension" />
    </declare-styleable>


    <declare-styleable name="CameraButton">
        <attr name="minTime" format="integer" />
        <attr name="maxTime" format="integer" />
        <attr name="progressColor" format="color" />
        <attr name="progressWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CameraScanBar">
        <attr name="duration" format="integer" />
        <attr name="startY" format="dimension" />
        <attr name="endY" format="dimension" />
        <attr name="startAlpha" format="float" />
        <attr name="endAlpha" format="float" />
    </declare-styleable>

    <declare-styleable name="CameraFocusBar">
        <attr name="duration_focus" format="integer"/>
        <attr name="fromScale" format="float"/>
        <attr name="toScale" format="float"/>
    </declare-styleable>

</resources>